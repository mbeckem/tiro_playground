{
    "name": "tiro_playground",
    "description": "The tiro programming language",
    "version": "0.1.0",
    "author": "Michael Beckemeyer",
    "dependencies": {},
    "keywords": [],
    "license": "MIT",
    "scripts": {
        "dev": "gatsby develop",
        "clean": "gatsby clean",
        "build": "gatsby build --prefix-paths",
        "serve": "gatsby serve --prefix-paths",
        "init-deploy": "git worktree add gh-deploy gh-pages",
        "deploy:fresh": "rm -rf public/* gh-deploy/*",
        "deploy:push": "cd gh-deploy && git add -A && git commit --allow-empty -m \"Deploy to gh-pages.\" && git push --force",
        "deploy": "npm run deploy:fresh && npm run build && cp -r public/* gh-deploy/ && npm run deploy:push",
        "check": "tsc -noEmit",
        "check:watch": "npm run check -- -watch"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@blueprintjs/core": "^3.30.1",
        "@loadable/component": "^5.13.1",
        "@types/classnames": "^2.2.10",
        "@types/loadable__component": "^5.13.0",
        "@types/node": "^13.13.15",
        "@types/react": "^16.9.44",
        "@types/react-helmet": "^5.0.15",
        "@types/react-resize-detector": "^4.2.0",
        "@typescript-eslint/eslint-plugin": "^2.29.0",
        "@typescript-eslint/parser": "^2.29.0",
        "ace-builds": "^1.4.12",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "classnames": "^2.2.6",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.20.5",
        "file-loader": "^6.0.0",
        "gatsby": "^2.24.23",
        "gatsby-alias-imports": "^1.0.4",
        "gatsby-image": "^2.4.14",
        "gatsby-plugin-manifest": "^2.4.21",
        "gatsby-plugin-offline": "^3.2.21",
        "gatsby-plugin-react-helmet": "^3.3.10",
        "gatsby-plugin-remove-console": "0.0.2",
        "gatsby-plugin-sass": "^2.3.12",
        "gatsby-plugin-sharp": "^2.6.24",
        "gatsby-plugin-typescript": "^2.4.16",
        "gatsby-plugin-webpack-size": "^1.0.0",
        "gatsby-source-filesystem": "^2.3.23",
